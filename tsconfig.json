{
  "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
			"@/*": ["./src/*"],
			"@/components": ["./src/components"],
			"@/components/*": ["./src/components/*"],
			"@/configs": ["./src/configs"],
			"@/configs/*": ["./src/configs/*"],
			"@/constants": ["./src/constants"],
			"@/constants/*": ["./src/constants/*"],
			"@/handlers": ["./src/handlers"],
			"@/handlers/*": ["./src/handlers/*"],
			"@/hooks": ["./src/hooks"],
			"@/hooks/*": ["./src/hooks/*"],
			"@/types": ["src/types"],
			"@/types/*": ["src/types/*"],
			"@/navigations": ["./src/navigations"],
			"@/navigations/*": ["./src/navigations/*"],
			"@/providers": ["./src/providers"],
			"@/providers/*": ["./src/providers/*"],
			"@/screens": ["./src/screens"],
			"@/screens/*": ["./src/screens/*"],
			"@/tailwind": ["./src/tailwind"],
			"@/utils": ["./src/utils"],
			"@/utils/*": ["./src/utils/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "gamblr.d.ts"],
  "exclude": ["node_modules"]
}
